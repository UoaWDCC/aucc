{
  "name": "aucc",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@10.5.2",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbopack",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "format": "prettier --write .",
    "prepare": "husky install",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@mdx-js/react": "^3.1.0",
    "@payloadcms/db-postgres": "3.30.0",
    "@payloadcms/next": "3.30.0",
    "@payloadcms/richtext-lexical": "3.30.0",
    "@payloadcms/storage-s3": "3.30.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tailwindcss/postcss": "^4.0.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "graphql": "^16.8.1",
    "lucide-react": "^0.477.0",
    "next": "15.2.3",
    "payload": "3.30.0",
    "postcss": "^8.5.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sharp": "0.32.6",
    "slugify": "^1.6.6",
    "styled-jsx": "^5.1.7",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.9",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "3",
    "@eslint/eslintrc": "^3.2.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-onboarding": "8.6.14",
    "@storybook/blocks": "8.6.14",
    "@storybook/experimental-addon-test": "8.6.14",
    "@storybook/experimental-nextjs-vite": "8.6.14",
    "@storybook/react": "8.6.14",
    "@storybook/test": "8.6.14",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.5.4",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "@vitest/browser": "3.1.2",
    "@vitest/coverage-v8": "^3.1.2",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.1.5",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-storybook": "^0.12.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.0",
    "msw": "^2.7.5",
    "playwright": "^1.52.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "storybook": "8.6.14",
    "typescript": "5.7.3",
    "vitest": "^3.1.2"
  },
  "resolutions": {
    "@payloadcms/*": "3.30.0"
  },
  "engines": {
    "node": "^22.14.0",
    "pnpm": "^10.5.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  },
  "volta": {
    "node": "22.14.0"
  },
  "lint-staged": {
    "*.{mjs,js,ts,tsx,json,md}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
